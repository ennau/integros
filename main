<script src="http://www.datejs.com/build/date.js"></script>
<script>
      // Date Format 24h: (YYYY, MM, DD, h, mm)
      // 0 month = January

      var start = new Date(Date.UTC(2017, 1, 9, 19, 00)).toString("MMMM dd,  hh:mm");
      document.getElementById("startDate").innerHTML = start;

      var end = new Date(Date.UTC(2017, 1, 9, 19, 45)).toString("hh:mm tt");
      document.getElementById("endDate").innerHTML = end;



      //Parser timezone
      var tz = /(GMT(\-|\+).*\(.*)\)/.exec(new Date().toString())[0];
      if (tz !== 'undefined') {
        document.getElementById("timezone").innerHTML = tz;
      } else {
        tz = 0
      }
</script>

